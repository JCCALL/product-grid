{
  "name": "product-grid",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCCALL/product-grid.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.4",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.5.2",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "helmet": "^4.1.1",
    "morgan": "^1.10.0",
    "mysql2": "^1.6.4",
    "path": "^0.12.7",
    "sequelize": "^4.41.2"
  },
  "bugs": {
    "url": "https://github.com/JCCALL/product-grid/issues"
  },
  "homepage": "https://github.com/JCCALL/product-grid#readme"
}
